{
  "name": "LinkedIn Education",
  "version": "2.0.0",
  "scope": "linkedin.education",
  "dialect": "json",
  "description": "Education history from LinkedIn profile including schools, degrees, and years attended",
  "schema": {
    "type": "object",
    "properties": {
      "education": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "schoolName": { "type": "string" },
            "degree": { "type": "string" },
            "years": { "type": "string" },
            "grade": { "type": "string" },
            "logoUrl": { "type": "string" }
          },
          "required": ["schoolName"],
          "additionalProperties": false
        }
      }
    },
    "required": ["education"],
    "additionalProperties": false
  }
}
