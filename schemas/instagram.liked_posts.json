{
  "name": "Instagram Liked Posts",
  "version": "1.0.0",
  "scope": "instagram.liked_posts",
  "dialect": "json",
  "description": "Posts liked by the user on Instagram",
  "schema": {
    "type": "object",
    "properties": {
      "liked_posts": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": { "type": "string" },
            "shortcode": { "type": "string" },
            "ownerUsername": { "type": "string" },
            "ownerId": { "type": "string" },
            "caption": { "type": "string" },
            "mediaType": { "type": "string", "enum": ["image", "video", "carousel"] },
            "mediaUrl": { "type": "string" },
            "thumbnailUrl": { "type": "string" },
            "timestamp": { "type": "string" },
            "likeCount": { "type": "number" },
            "commentCount": { "type": "number" }
          },
          "required": ["id", "shortcode", "ownerUsername", "ownerId", "caption", "mediaType", "mediaUrl", "timestamp"],
          "additionalProperties": false
        }
      }
    },
    "required": ["liked_posts"],
    "additionalProperties": false
  }
}
